{
    "name": "Traefik (Beta)",
    "version": "0.5.2",
    "slug": "traefik-beta",
    "description": "Reverse Proxy for Web Services",
    "arch": ["armv7", "aarch64", "amd64"],
    "startup": "system",
    "boot": "auto",
    "image": "pheelee/hassio-addon-traefik-{arch}",
    "ports": {
        "80/tcp": 80,
        "443/tcp": 443
    },
    "hassio_api": false,
    "hassio_role": "default",
    "map": [],
    "options": {
        "loglevel": "error",
        "email": "",
        "httpreq": {
            "url": "",
            "username": "",
            "password": ""
        },
        "authEndpoint": "",
        "cookieSecret": "your-super-secure-string-here",
        "insecureSkipVerify": false,
        "hosts":[],
        "rootCAs": []
    },
    "ingress": true,
    "panel_icon":"mdi:routes",
    "schema": {
        "loglevel": "list(debug|panic|fatal|error|warn|info)",
        "email": "email",
        "httpreq": {
            "url": "str?",
            "username": "str?",
            "password": "str?"
        },
        "authEndpoint": "str?",
        "cookieSecret": "str?",
        "insecureSkipVerify": "bool",
        "hosts": ["str"],
        "rootCAs": ["str"]
    }
  }
